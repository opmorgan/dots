# set environmental variables
export XDG_CONFIG_HOME="$HOME/.config"
export R_ENVIRON_USER="$HOME/.config/r/.Renviron"
export TEST1="999899"

# set $PATH
export PATH="/usr/local/bin:/usr/local/sbin:/usr/bin:/usr/bin/site_perl:/usr/bin/vendor_perl:/usr/bin/core_perl:/home/om/bin:/home/om/.cargo/bin"

# set aliases
alias lsa='ls -a'
alias lla="ll -a"
alias tp='trash-put'
alias R='R --quiet --no-save'
source /usr/share/nvm/init-nvm.sh
